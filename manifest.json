{
  "manifest_version": 2,
  "name": "qbta",
  "description": "Qiita stock and hatena bookmark at the same time",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "http://qiita.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://qiita.com/*"],
      "js": ["myscript.js"],
      "run_at": "document_end"
    }
  ]
}
